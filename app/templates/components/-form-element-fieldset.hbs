{{#if isEditing}}
<div class="field fieldset edit" data-id={{ formElement.id }}>
    <div class="row">
        <h3>{{ formElement.getElementTypeDisplay }}</h3>
    </div>
    {{#if isLabelEmpty}}
    <div class="row">
        <strong>Please insert at least a label.</strong>
    </div>
    {{/if}}
    <div class="row">
        <label>Title:</label> <span>{{input value=formElement.label }}</span>
    </div>
    <div class="row">
        <label>Immutable:</label> <span>{{input type="checkbox" checked=formElement.immutable}}</span>
    </div>
    {{ partial "components/-form-element-save-row" }}
</div>
{{else}}
<div class="field fieldset {{if formElement.immutable 'immutable'}}" data-id={{ formElement.id }}>
    <h2 {{action "togglIsEditing" on="doubleClick"}}>{{ formElement.label }}</h2>
    {{dragable-elements formElements=formElement.getFormElements store=store}}
</div>
{{/if}}
