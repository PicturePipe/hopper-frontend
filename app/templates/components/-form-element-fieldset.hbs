{{#if isEditing}}
<div class="field edit" data-id={{ formElement.id }}>
    <div class="row">
        <label>Title:</label> <span>{{input value=formElement.label }}</span>
    </div>
    <div class="row">
        <label>Immutable:</label> <span>{{input type="checkbox" checked=formElement.immutable}}</span>
    </div>
    <div class="row save-row">
        <a {{ action "togglIsEditing" on="click"}}>
            <i class="fa fa-check-circle"></i>
        </a>
    </div>
</div>
{{else}}
<div class="field {{if formElement.immutable 'immutable'}}" data-id={{ formElement.id }}>
    <h2 {{action "togglIsEditing" on="doubleClick"}}>{{ formElement.label }}</h2>
    {{dragable-elements formElements=formElement.getFormElements store=store}}
</div>
{{/if}}
